   -
     name : CreeArborescence
     procedure_id : 1948853195957342813
     type_code : 14
     code : |1+
      // Résumé : Répertoire dont il faut lister le contenu
      // Syntaxe :
      //CreeArborescence (<sChemin> est chaîne)
      //
      // Paramètres :
      //	sChemin (chaîne) : <indiquez ici le rôle de sChemin>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE CreeArborescence(sChemin est une chaine)
      
      // Variable
      gsCheminDisque = sChemin
      gsCheminDisque = DécomplèteRep(gsCheminDisque)
      
      // Fenêtre patience
      OuvreFille(FEN_Patience)
      FEN_Patience.LIB_FICHIER = sChemin
      
      // Réinitialisation des champs
      TableSupprimeTout(TABLEH_TableFichier)
      TreeMapSupprimeTout(TMAP_Fichier)
      
      // Création de la racine
      TableAjouteLigne(TABLEH_TableFichier, gsCheminDisque)
      
      // Recherche des fichiers et sous-répertoires
      fListeFichier(ComplèteRep(gsCheminDisque) + "*.*", ListeFichier, Null, frInterruptible)
      
      // Déroule tout
      TableDérouleTout(TABLEH_TableFichier)
      
      Ferme(FEN_Patience)
      
      
     type : 458752
   -
     name : RecupereChemin
     procedure_id : 1948853539554806413
     type_code : 14
     code : |1+
      // Résumé : Récupère le chemin à partir d'un indice du TreeMap
      // Syntaxe :
      // [ <Résultat> = ] RecupereChemin (<nIndice>)
      //
      // Paramètres :
      // 	nIndice : <indiquez ici le rôle de nIndice>
      // Valeur de retour :
      // 	chaîne : <indiquez ici les valeurs possibles ainsi que leur interprétation>
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE RécupèreChemin(LOCAL nIndice)
      
      sChemin est une chaine = TABLEH_TableFichier.COL_Nom[nIndice]
      
      TANTQUE nIndice <> 0 ET nIndice <> -1
      	// Recherche de l'indice du parent
      	nIndice = TableRécupèreParent(TABLEH_TableFichier, nIndice)
      	SI nIndice <> 0 ET nIndice <> -1 ALORS
      		sChemin = TABLEH_TableFichier.COL_Nom[nIndice] + ["\"] + sChemin
      	FIN
      FIN
      
      // On ignore le répertoire <Fichiers> qui sert juste visuellement
      sChemin = Remplace(sChemin, "\<Fichiers>", "")
      
      RENVOYER sChemin
     type : 458752
   -
     name : ListeFichier
     internal_properties : CAAAAAgAAABKXJr3lgFN6X++thTbIlm9ODbHVEqlke21+pBjA8PSjgQnmUiCKbmDW1eM6AWqKLJZxNQ9GzD1swb8bzpNIIi652nFwVQBCM3pAioioF/T6StdVMR3Nr4OfU63AHtCawHe4KgO3v923neupqKnWdmVfxgVrh0aJHrcRXqvkEZzM15d5giiNaPN2AmpME3T+n1q0wLbaw==
     procedure_id : 1948853664108952642
     type_code : 14
     code : |1+
      // Résumé : Callback appellée par la fonction fListeFichier
      // Syntaxe :
      // ListeFichier (<sChemin>, <sNomFichier>, <nChange>)
      //
      // Paramètres :
      // 	sChemin : <indiquez ici le rôle de sChemin>
      // 	sNomFichier : <indiquez ici le rôle de sNomFichier>
      // 	nChange : <indiquez ici le rôle de nChange>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ListeFichier(sChemin, sNomFichier, nChange)
      
      // Variables
      sParent, sArborescence est une chaine
      nIndice est un entier
      
      Multitâche(-1)
      FEN_Patience.LIB_FICHIER = sChemin + sNomFichier
      
      SELON nChange
      	CAS flPremierFichier
      		// Ajout du répertoire "<Fichiers>"
      		TableAjouteFils(TABLEH_TableFichier, gsCheminDisque, "<Fichiers>")
      		
      		sParent = TAB + "<Fichiers>"
      		
      	CAS flChangeRépertoire
      		// Ajout du répertoire
      		sParent = Remplace(sChemin, gsCheminDisque + "\", "", SansCasse)	// On supprime la racine du chemin
      		sParent = DécomplèteRep(sParent)
      		AjouteRépertoire(sParent)
      		
      		// Ajout du répertoire "<Fichiers>"
      		TableAjouteFils(TABLEH_TableFichier, gsCheminDisque + TAB + sParent, "<Fichiers>")
      		
      		sParent = TAB + sParent + TAB + "<Fichiers>"
      		
      	CAS flFichier
      		// Recherche de l'arborescence du parent
      		sParent = Remplace(sChemin, gsCheminDisque, "", SansCasse)	// On supprime la racine du chemin
      		sParent = DécomplèteRep(sParent)
      		sParent = Remplace(sParent, "\", TAB)
      		sParent += TAB+"<Fichiers>"
      FIN
      
      // Ajout du fichier
      nIndice = TableAjouteFils(TABLEH_TableFichier, gsCheminDisque + sParent, sNomFichier, fTaille(sChemin + sNomFichier))
      
      // Modification de l'icône de l'élément ajouté
      TABLEH_TableFichier[nIndice]..ImageEnroulée = IMG_Fichier
      TABLEH_TableFichier[nIndice]..ImageDéroulée = IMG_Fichier
      
      // MAJ du TreeMap
      sArborescence = Remplace(gsCheminDisque + sParent, TAB, "\")
      sArborescence = Remplace(sArborescence, "<Fichiers>", sNomFichier)
      
      TreeMapAjoute(TMAP_Fichier, sArborescence, TABLEH_TableFichier.COL_Taille[nIndice])
      
      
     type : 458752
   -
     name : AjouteRépertoire
     procedure_id : 1948853947576860260
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AjouteRépertoire (<sArborescence>)
      //
      // Paramètres :
      //	sArborescence : <indiquez ici le rôle de sArborescence>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE AjouteRépertoire(sArborescence)
      
      // Variables
      nIndiceParent, nIndiceAjouté est un entier
      sArborescenceTMP est une chaine
      nIndiceParent = 1
      sArborescence = Remplace(sArborescence, "\", TAB)
      
      POUR TOUTE CHAINE sRépertoire DE sArborescence SEPAREE PAR TAB
      	nIndiceParent = TableChercheFils(COL_Nom, sRépertoire, Vrai, nIndiceParent)
      	
      	SI nIndiceParent = -1 ALORS
      		nIndiceAjouté = TableAjouteFils(TABLEH_TableFichier, gsCheminDisque + [TAB] + sArborescenceTMP, sRépertoire)
      		nIndiceParent = nIndiceAjouté
      	SINON
      		nIndiceAjouté = nIndiceParent
      	FIN
      	
      	// construction de l'arborescence
      	sArborescenceTMP += [TAB] + sRépertoire
      FIN
     type : 458752
   -
     name : CherchePositionTable
     procedure_id : 1948865166025333205
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // CherchePositionTable (<sChemin>)
      //
      // Paramètres :
      // 	sChemin : <indiquez ici le rôle de sChemin>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE CherchePositionTable(LOCAL sChemin)
      
      // Variables
      nIndiceParent est un entier = 1
      
      // Récupération du chemin seulement
      sChemin = fExtraitChemin(sChemin,fDisque+fRépertoire) + "<Fichiers>\" + fExtraitChemin(sChemin,fFichier+fExtension)
      
      // Suppression du début du chemin
      sChemin = Remplace(sChemin, ComplèteRep(gsCheminDisque), "")
      
      // Extraction du répertoire à rechercher
      POUR TOUTE CHAINE sRépertoire DE sChemin SEPAREE PAR "\"
      	nIndiceParent = TableChercheFils(COL_Nom,sRépertoire,Vrai,nIndiceParent)
      FIN
      
      TableEnrouleTout(TABLEH_TableFichier)
      TableSelectPlus(TABLEH_TableFichier,nIndiceParent)
      TablePosition(TABLEH_TableFichier,nIndiceParent)
      
     type : 458752
   -
     name : TailleFormate
     internal_properties : CAAAAAgAAADaWmN/i8nNK7yvrCpzuMfseZRX0WqDW3+qqiaGlVVsvQyEoIyvBIqydOrLjKBt+c/uL2GIt6e/3vFkCLPyTpA6M9ekSdIBn+PoJ0fTlfe5kWhTFNll+IUi7NmUgr0OUC3PJk3/NIdzLG4Bn7zFI0Rt5FLQYbO4mzApliXZvmLCs+Isbh5TfMWwqgfJO57mM4io3n8u2SH/t74V+7sAlycxBwA47CS9
     procedure_id : 1948868546164661801
     type_code : 14
     code : |1+
      // Résumé : Formate un entier en chaine lisible
      // Syntaxe :
      //[ <Résultat> = ] TailleFormate (<nTaille> est entier sans signe sur 4 octets)
      //
      // Paramètres :
      //	nTaille (entier sans signe sur 4 octets) : <indiquez ici le rôle de nTaille>
      // Valeur de retour :
      // 	chaîne : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE TailleFormate(LOCAL nTaille est un entier sans signe sur 4 octets)
      
      // Taille
      rTaille est un réel
      sTaille est une chaine
      
      SI nTaille < 1024 ALORS // Octets
      	rTaille = nTaille / 1024
      	sTaille = NumériqueVersChaîne(rTaille, "4,1f") + " octets"	
      SINON SI nTaille < 1024 * 1024 // Kilos Octets
      	rTaille = nTaille / 1024
      	sTaille = NumériqueVersChaîne(rTaille, "4,1f") + " Ko"
      SINON SI nTaille < 1024 * 1024 * 1024 // Mega octets
      	rTaille = (nTaille / 1024) / 1024
      	sTaille = NumériqueVersChaîne(rTaille, "4,1f") + " Mo"
      SINON SI nTaille < 1024 * 1024 * 1024 * 1024 // Giga octets
      	rTaille = ((nTaille / 1024) / 1024) / 1024
      	sTaille = NumériqueVersChaîne(rTaille, "4,1f") + " Go"
      SINON
      	dbgAssertion(Faux,"Valeur de taille impossible")
      FIN
      
      RENVOYER sTaille
      
